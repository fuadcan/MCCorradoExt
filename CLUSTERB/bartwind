/*
output on;
output file=c:\gauss32\out\"c1%*.*lf".dat reset;
print c1;
output off;
cc=ftos(y,"c1%*.*lf",1,0);
print bandwidt~y;

k=rows(c1);
M= zeros(u*k,u*k);
y=0;
do while ( y < u );
    y =y+1;
i=0;
do while ( y*k+1< i < y*k+k);
    i =i+1;
    j = 0;
    do while (y*k+1<  j < y*k+k);
        j = j+1;
      
    M[i,j] = (c1[i,y] - c1[j,y] == 0);

*/


e=0;
ind=zeros(6,1);
do while ( e <6 );
    e = e+1;
ind[e]=e;
endo;

u=rows(ind);
y=0;
i=0;
j=0;
k=68;
m=zeros(k,u);
n=zeros(k,u);
do while ( y < u);
    y = y+1;
bandwidt=y;
#include "c:\gauss32\cluster\tivarfin";
m[1:k,y]=c1;
endo;


i=0;
M1= zeros(k,k);
do while ( i < k );
    i = i+1;

    j = 0;
    do while ( j < k );
        j = j+1;
      
    M1[i,j] = (m[i,1] - m[j,1] == 0);
         
    endo; @ j @
endo; @ i @

i=0;
M2= zeros(k,k);
do while ( i < k );
    i = i+1;

    j = 0;
    do while ( j < k );
        j = j+1;
      
    M2[i,j] = (m[i,2] - m[j,2] == 0);
         
    endo; @ j @
endo; @ i @


i=0;
M3= zeros(k,k);
do while ( i < k );
    i = i+1;

    j = 0;
    do while ( j < k );
        j = j+1;
      
    M3[i,j] = (m[i,3] - m[j,3] == 0);
         
    endo; @ j @
endo; @ i @


i=0;
M4= zeros(k,k);
do while ( i < k );
    i = i+1;

    j = 0;
    do while ( j < k );
        j = j+1;
      
    M4[i,j] = (m[i,4] - m[j,4] == 0);
         
    endo; @ j @
endo; @ i @



i=0;
M5= zeros(k,k);
do while ( i < k );
    i = i+1;

    j = 0;
    do while ( j < k );
        j = j+1;
      
    M5[i,j] = (m[i,5] - m[j,5] == 0);
         
    endo; @ j @
endo; @ i @

i=0;
M6= zeros(k,k);
do while ( i < k );
    i = i+1;

    j = 0;
    do while ( j < k );
        j = j+1;
      
    M6[i,j] = (m[i,6] - m[j,6] == 0);
         
    endo; @ j @
endo; @ i @




format 1,4;

C=zeros(u,u);
t12 = ((sumc(vech(M1  .*  M2))-k) / ((sumc(vech(M1))- k)^0.5*(sumc(vech(M2))- k)^0.5))^0.5;
t13 = ((sumc(vech(M1  .*  M3))-k) / ((sumc(vech(M1))- k)^0.5*(sumc(vech(M3))- k)^0.5))^0.5;
t14 = ((sumc(vech(M1  .*  M4))-k) / ((sumc(vech(M1))- k)^0.5*(sumc(vech(M4))- k)^0.5))^0.5;
t15 = ((sumc(vech(M1  .*  M5))-k) / ((sumc(vech(M1))- k)^0.5*(sumc(vech(M5))- k)^0.5))^0.5;
t16 = ((sumc(vech(M1  .*  M6))-k) / ((sumc(vech(M1))- k)^0.5*(sumc(vech(M6))- k)^0.5))^0.5;

C[1,2]=t12;
C[1,3]=t13;
C[1,4]=t14;
C[1,5]=t15;
C[1,6]=t16;


t23 = ((sumc(vech(M2  .*  M3))-k) / ((sumc(vech(M2) )- k)^0.5*(sumc(vech(M3) )- k)^0.5))^0.5;
t24 = ((sumc(vech(M2  .*  M4))-k) / ((sumc(vech(M2) )- k)^0.5*(sumc(vech(M4) )- k)^0.5))^0.5;
t25 = ((sumc(vech(M2 .*  M5))-k) / ((sumc(vech(M2) )- k)^0.5*(sumc(vech(M5) )- k)^0.5))^0.5;
t26 = ((sumc(vech(M2  .*  M6))-k) / ((sumc(vech(M2) )- k)^0.5*(sumc(vech(M6) )- k)^0.5))^0.5;

C[2,3]=t23;
C[2,4]=t24;
C[2,5]=t25;
C[2,6]=t26;



t34 = ((sumc(vech(M3  .*  M4))-k) / ((sumc(vech(M3) )- k)^0.5*(sumc(vech(M4) )- k)^0.5))^0.5;
t35 = ((sumc(vech(M3  .*  M5))-k) / ((sumc(vech(M3) )- k)^0.5*(sumc(vech(M5) )- k)^0.5))^0.5;
t36 = ((sumc(vech(M3  .*  M6))-k) / ((sumc(vech(M3) )- k)^0.5*(sumc(vech(M6) )- k)^0.5))^0.5;

C[3,4]=t34;
C[3,5]=t35;
C[3,6]=t36;



t45 = ((sumc(vech(M4  .*  M5))-k) / ((sumc(vech(M4))- k)^0.5*(sumc(vech(M5) )- k)^0.5))^0.5;
t46 = ((sumc(vech(M4  .*  M6))-k) / ((sumc(vech(M4))- k)^0.5*(sumc(vech(M6) )- k)^0.5))^0.5;

C[4,5]=t45;
C[4,6]=t46;



t56 = ((sumc(vech(M5  .*  M6))-k) / ((sumc(vech(M5) )- k)^0.5*(sumc(vech(M6) )- k)^0.5))^0.5;

C[5,6]=t56;
print  C;

