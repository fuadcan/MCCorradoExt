do while ( i < k );
    i = i+1;

    j = 0;
    do while ( j < k );
        j = j+1;
      
    M1B[i,j] = (c1[i] - c1[j] == 0);
         
    endo; @ j @
endo; @ i @



/* Now compare Mperi with Mhat*/

gmanu = ((sumc(vech(M1B  .*  M1H))-k) / ((sumc(vech(M1B))- k)^0.5*(sumc(vech(M1H) )- k)^0.5))^0.5;






@----------------------------------------------------------------------------------------------------------------------------@
format  1,3;

print " gag gmanu gms gnms" gag~gmanu~gms~gnms; 

format  1,4;


