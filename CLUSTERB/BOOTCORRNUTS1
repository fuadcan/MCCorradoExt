do while ( i < k );
    i = i+1;

    j = 0;
    do while ( j < k );
        j = j+1;
      
    M1B[i,j] = (c1[i] - c1[j] == 0);
         
    endo; @ j @
endo; @ i @



/* Now compare Mperi with Mhat*/

gag = ((sumc(vech(M1B  .*  M1H))-k) / ((sumc(vech(M1B))- k)^0.5*(sumc(vech(M1H) )- k)^0.5))^0.5;







@---------------------------------------------------------------------------------------------------------@
load c0[k,1] = f:\dae\lc242\clusterb\output\c1manhf.dat ;

format  6,0;
i = 0;

@Transform c0 in matrix format@

